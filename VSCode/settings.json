{
    "files.exclude": {
        "bin/": true,
        "obj/": true,
        "node_modules": true
    },
    "telemetry.enableTelemetry": false,
    "editor.codeLens": false,
    "git.enableSmartCommit": true, // Automatically stage when committing
    "git.confirmSync": false, // Don't show dialog for git push
    "window.zoomLevel": 0,
    "editor.wordWrap": "off",
    "emmet.triggerExpansionOnTab": true,
    "breadcrumbs.enabled": true
}